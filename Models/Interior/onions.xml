<?xml version="1.0"?>

<PropertyList>

 <path>onions.ac</path>

 <animation>
  <type>material</type>
  <object-name>bunch</object-name>
  <emission>
   <red>1.0</red>
   <green>0.2</green>
   <blue>0.0</blue>
   <factor-prop>/sim/model//material/instruments/factor</factor-prop>
  </emission>
 </animation>

 
  <animation>
    <type>rotate</type>
    <object-name>bunch</object-name>
    <property>/orientation/roll-deg</property>
    <factor>0.7</factor>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>
   <animation>
    <type>rotate</type>
    <object-name>bunch</object-name>
    <property>/orientation/pitch-deg</property>
    <factor>0.7</factor>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
    <animation>
    <type>rotate</type>
    <object-name>bunch</object-name>
    <property>/orientation/yaw-rate-degps</property>
    <factor>-5</factor>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
  </animation>

</PropertyList>
